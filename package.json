  "devDependencies": {
    "@applint/spec": "^1.0.0",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@iceworks/spec": "^1.6.0",
    "babel-loader": "^9.2.1",
    "eslint": "^8.0.0",
    "prettier": "^2.8.8",
    "stylelint": "^16.10.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
