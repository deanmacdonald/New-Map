{
  "name": "leaflet-map-project",
  "version": "1.1.0",
  "description": "A project to display a Leaflet map with multiple locations",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "build": "webpack --config webpack.config.js",
    "deploy": "pm2 start server.js",
    "eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "stylelint": "stylelint '**/*.{css,scss,less}'",
    "prettier": "prettier **/* --write",
    "lint": "npm run eslint && npm run stylelint",
    "test-build": "npm test && npm run build",
    "clean": "rm -rf dist && mkdir dist"
  },
  "author": "Dean Macdonald",
  "license": "MIT",
  "dependencies": {
    "@waxio/waxjs": "^0.0.15",
    "async": "^3.2.6",
    "axios": "^1.7.7",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.3",
    "buffer": "^6.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "deployment": "^0.0.2",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "errors": "^0.3.0",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.1",
    "five": "^0.8.0",
    "helmet": "^8.0.0",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-routing-machine": "^3.2.12",
    "leaflet.markercluster": "^1.5.3",
    "lint": "^1.1.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1",
    "pm2": "^5.1.0",
    "server": "^0.0.3",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.4",
    "web": "^0.0.2"
