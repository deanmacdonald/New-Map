{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./src/components",
          "@pages": "./src/pages",
          "@styles": "./client-side/public/styles"
        }
      }
    ]
  ]
}

